<h1>New source</h1>

<% setup_sources(@source).each do |source| %>
  <% form_for [ container, source ] do |f| %>
    <%= f.error_messages %>

    <%= render :partial => 'form', :object => f %>

    <p>
      <%= f.submit 'Create' %>
    </p>
  <% end %>
<% end %>

<%= link_to 'Back', sources_path %>
